{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Build executor",
  "description": "",
  "type": "object",
  "properties": {
    "root": {
      "type": "string",
      "description": "vite project root"
    },
    "configFile": {
      "type": "string",
      "description": "vite config file path"
    },
    "outDir": {
      "type": "string",
      "default": "dist"
    },
    "watch": {
      "type": "boolean",
      "default": false
    },
    "write": {
      "type": "boolean",
      "default": true
    },
    "emitAtRootLevel": {
      "type": "boolean",
      "default": true
    },
    "manifest": {
      "type": "boolean",
      "default": true
    }
  },
  "required": ["configFile"]
}
